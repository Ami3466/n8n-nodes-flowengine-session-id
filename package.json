{
	"name": "n8n-nodes-flowengine-ai-session-manager",
	"version": "1.0.0",
	"description": "n8n community node for generating and managing Session IDs for AI Agents. Solves the 'Setup Friction' and 'Loop Amnesia' problems.",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ai",
		"session",
		"uuid",
		"agent",
		"memory",
		"workflow"
	],
	"license": "MIT",
	"homepage": "https://github.com/flowengine/n8n-nodes-flowengine-ai-session-manager",
	"author": {
		"name": "FlowEngine",
		"email": "support@flowengine.ai"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/flowengine/n8n-nodes-flowengine-ai-session-manager.git"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"main": "dist/nodes/AiSessionManager/AiSessionManager.node.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes --ext .ts --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"nodes": [
			"dist/nodes/AiSessionManager/AiSessionManager.node.js"
		],
		"credentials": []
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.22.0",
		"prettier": "^3.1.0",
		"typescript": "^5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
